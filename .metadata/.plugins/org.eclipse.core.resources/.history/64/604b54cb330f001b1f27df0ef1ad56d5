package com.rui.zipcode;

import static org.junit.Assert.assertEquals;

import java.util.ArrayList;
import java.util.Arrays;
import java.util.List;

import org.junit.*;
import static com.rui.zipcode.ZipCodePreProcess.*;

public class ZipCodePreProcessTest {
	

	@Test
	public void generateZipCodeArrayTest() {
		
		
		int[][] nums = {{10000, 20000},{30000, 40000},{50000, 60000}};
		List<int[]> tar = Arrays.asList(nums);
		tar.stream().forEach(t -> System.out.println(Arrays.toString(t)));
		
		List<int[]> res = generateZipCodeArray("[10000,20000] [30000,40000] [50000,60000]");
		res.stream().forEach(t -> System.out.println(Arrays.toString(t)));
		assert(res == tar);

	}
	
//	@Test(expected = IllegalArgumentException.class)
//	public void catchMissingValue() {
//		String[] zip = {""}
//	}
}
