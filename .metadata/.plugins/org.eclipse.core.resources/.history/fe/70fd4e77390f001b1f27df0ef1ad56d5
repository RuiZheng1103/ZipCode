package com.rui.zipcode;

import org.junit.*;
import static com.rui.zipcode.ZipCodeMerge.*;
import static com.rui.zipcode.ZipCodePreProcess.generateZipCodeArray;

import java.util.Arrays;
public class ZipCodeMergeTest {

	@Test(expected = IllegalArgumentException.class)
	public void illegalInput() {
		merge(null);
	}
	
	@Test
	public void generateZipCodeArrayTest() {
		int[][] nums = {{10000, 20000},{30000, 40000},{50000, 60000}};		
		int[][] res = generateZipCodeArray("[10000,20000] [30000,40000] [50000,60000]");	
		assert(Arrays.deepEquals(res, nums));

	}
}
